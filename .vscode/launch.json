{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Flask",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "plos_exp.py",
        "FLASK_DEBUG": "1"
      },
      "args": [
        "run",
        "--host",
        "0.0.0.0",
        "--port",
        "5000"
      ],
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "d:/OneDrive-UofT/OneDrive - University of Toronto/LAIR_Tobii_EyeGaze/v2/etta-tobii-v2",
      "program": "d:/OneDrive-UofT/OneDrive - University of Toronto/LAIR_Tobii_EyeGaze/v2/etta-tobii-v2/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}